<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_nav"
    app:startDestination="@id/adminPanelFragment">

    <fragment
        android:id="@+id/adminPanelFragment"
        android:name="com.cubbysulotions.cliurches.Admin.AdminPanelFragment"
        android:label="fragment_admin_panel"
        tools:layout="@layout/fragment_admin_panel" >
        <action
            android:id="@+id/action_adminPanelFragment_to_massScheduleFragment"
            app:destination="@id/massScheduleFragment" />
        <action
            android:id="@+id/action_adminPanelFragment_to_pendingPaymentFragment"
            app:destination="@id/pendingPaymentFragment"/>
        <action
            android:id="@+id/action_adminPanelFragment_to_completedPaymentFragment"
            app:destination="@id/completedPaymentFragment" />
    </fragment>
    <fragment
        android:id="@+id/massScheduleFragment"
        android:name="com.cubbysulotions.cliurches.Admin.MassScheduleFragment"
        android:label="fragment_mass_schedule"
        tools:layout="@layout/fragment_mass_schedule" >
        <action
            android:id="@+id/action_massScheduleFragment_to_adminPanelFragment"
            app:destination="@id/adminPanelFragment" />
    </fragment>
    <fragment
        android:id="@+id/pendingPaymentFragment"
        android:name="com.cubbysulotions.cliurches.Admin.PendingPaymentFragment"
        android:label="fragment_pending_payment"
        tools:layout="@layout/fragment_pending_payment" >
        <action
            android:id="@+id/action_pendingPaymentFragment_to_adminPanelFragment"
            app:destination="@id/adminPanelFragment" />
    </fragment>
    <fragment
        android:id="@+id/completedPaymentFragment"
        android:name="com.cubbysulotions.cliurches.Admin.CompletedPaymentFragment"
        android:label="fragment_completed_payment"
        tools:layout="@layout/fragment_completed_payment" >
        <action
            android:id="@+id/action_completedPaymentFragment_to_adminPanelFragment"
            app:destination="@id/adminPanelFragment"
            />
    </fragment>
</navigation>